{
    "name": "amneale/dns-updater",
    "description": "DNS updater",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Neale",
            "email": "adam@adam-neale.co.uk"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "beberlei/assert": "^3.2",
        "guzzlehttp/guzzle": "^6.3",
        "jamesryanbell/cloudflare": "^1.11",
        "symfony/console": "^4.1",
        "toin0u/digitalocean-v2": "^2.2"
    },
    "require-dev": {
        "ext-xdebug": "*",
        "behat/behat": "^3.3",
        "friends-of-phpspec/phpspec-code-coverage": "^4.3",
        "friendsofphp/php-cs-fixer": "^2.12",
        "phpspec/phpspec": "^5.1",
        "roave/security-advisories": "dev-master"
    },
    "autoload": {
        "psr-4": {
            "DnsUpdater\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fake\\": "features/bootstrap/fake"
        }
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    }
}
